<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">
	<bean id="bd1" class="prueba06.Matematica"/>
	<bean class="prueba06.MateServicios">
		<property name="mate" ref="bd1"/>
		<property name="curso" value="Aplicaciones con Spring"/>
		<property name="lista">
			<list>
				<value>Java</value>
				<value>Python</value>
				<value>PHP</value>
				<value>Oracle</value>
			</list>
		</property>
	</bean>
</beans>